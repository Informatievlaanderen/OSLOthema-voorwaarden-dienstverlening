<trial certificate> a cpsv:PublicService ;
   dct:title "Test certificate"@nb ; # name
   cv:holdsRequirement <requirementsForMarriage> ; # has requirements
   cv:hasInput <examplePass> ; # has documentation requirements
   .

<criterionForMarriage> a cv:Criterion ;
   dct:title "Conditions for entering into marriage"@nb ; # name
   cv:isDerivedFrom [ a cv:ReferenceFramework ; # is derived from
      dct:identifier <https://lovdata.no/dokument/NL/lov/1991-07-04-47#KAPITTEL_1> ; ] ;
   cv:hasRequirement <age requirement> ; # has more specific requirements
   .

<alderskrav> a cv:InformationRequirement ;
   dct:title "Age requirements"@nb ; # name
   cv:isDerivedFrom [ a cv:ReferenceFramework ; # is derived from
      dct:identifier <https://lovdata.no/lov/1991-07-04-47/ยง1a> ; ] ;
   cv:hasRequirement <notUnder18> ; # has more specific requirements
   cv:hasEvidenceTypeList <typeListPass> ; # has documentation type list
   .

<ikkeUnder18> a cv:Constraint ;
   dct:title "Not under 18" ; # name
   cv:isDerivedFrom [ a cv:ReferenceFramework ; # is derived from
      dct:identifier <https://lovdata.no/lov/1991-07-04-47/ยง1a> ; ] ;
   cv:constrains <age> ; # limits
   .

<alder> a cv:InformationConcept ;
   dct:title "Alder"@nb ; # navn
   .

<typeListePass> a cv:EvidenceTypeList ;
   skos:prefLabel "pass"@nb ;
   cv:specifiesEvidenceType <typePass> ; #specifies
   .

<typePass> a cv:EvidenceType ;
   cv:evidenceTypeClassification <pass> ; # dokumentasjonstypekategori
   .

<pass> a skos:Concept ;
   skos:prefLabel "pass"@nb ; # navn
   .

<eksempelPass> a cv:Evidence ;
   dct:title "passport from the user as documentation"@nb ; # name
   dct:conformsTo <pass> ; # in accordance with
   cv:supportsConcept <age> ; # supports information concept
   cv:supportsRequirement <age requirement> ; # supports requirements
   cv:supportsValue <ageFromPass> ; # provides supporting information
   .

<alderFraPass> a cv:SupportedValue ;
   cv:providesValueFor <age> ; # gives value for
   cv:value "20"^^xsd:positiveInteger ; # verdi
   .